            "outputFile": "./storybook/storyLoader.js"
        }
    },
    "dependencies": {
        "expo": "~36.0.0",
        "react": "~16.9.0",
        "react-dom": "~16.9.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
        "react-native-web": "~0.11.7",
        "styled-components": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@storybook/addon-actions": "^5.3.8",
        "@storybook/addon-knobs": "^5.3.8",
        "@storybook/addon-ondevice-actions": "^5.3.8",
        "@storybook/addon-ondevice-knobs": "^5.3.8",
        "@storybook/addon-ondevice-notes": "^5.3.8",
        "@storybook/react-native": "^5.3.8",
        "@storybook/react-native-server": "^5.3.8",
        "babel-loader": "^8.0.6",
        "babel-preset-expo": "~8.0.0",
        "react-native-storybook-loader": "^1.8.1"
