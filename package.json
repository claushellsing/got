{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "prestorybook": "rnstl",
        "storybook": "start-storybook",
        "storybook-android": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook"
